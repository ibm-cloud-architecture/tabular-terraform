{
  "loadbalancers": [
    {
      "name*": "lb1",
      "subnets*": "subnet1,subnet2",
      "type": "public",
      "resource_group": null,
      "tags": null,
      "create_timeout": "60m",
      "delete_timeout": "60m"
    }
  ],
  "lblisteners": [
    {
      "name*": "lb1listener",
      "lb*": "lb1",
      "port*": 9080,
      "protocol*": "http",
      "default_pool": null,
      "certificate_instance": null,
      "connection_limit": null,
      "create_timeout": "60m",
      "update_timeout": "60m",
      "delete_timeout": "60m"
    }
  ],
  "lbpolicies": [
    {
      "name*": "lb1policy",
      "lb*": "lb1",
      "listener*": "lb1listener",
      "action*": "forward",
      "priority*": "2",
      "target_id": null,
      "target_http_status_code": null,
      "target_url": null
    }
  ],
  "lbrules": [
    {
      "name*": "lb1rule",
      "lb*": "lb1",
      "listener*": "lb1listener",
      "policy*": "lb1policy",
      "condition*": "contains",
      "type*": "header",
      "value*": "1",
      "file*": "2",
      "create_timeout": "10m",
      "update_timeout": "10m",
      "delete_timeout": "10m"
    }
  ],
  "lbpools": [
    {
      "name*": "lb1pool",
      "lb*": "lb1",
      "algorithm*": "round_robin",
      "protocol*": "http",
