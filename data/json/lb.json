      "update_timeout": "60m",
      "delete_timeout": "60m"
    }
  ],
  "lbmembers": [
    {
      "name*": "lb1member1",
      "pool*": "lb1pool",
      "lb*": "lb1",
      "port*": 8080,
      "target_address*": "vsi1",
      "weight": 60,
      "create_timeout": "60m",
      "update_timeout": "60m",
      "delete_timeout": "60m"
    },
    {
      "name*": "lb1member2",
      "pool*": "lb1pool",
      "lb*": "lb1",
      "port*": 8080,
      "target_address*": "vsi2",
